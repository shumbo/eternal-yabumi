{
  "manifest_version": 2,
  "name": "Eternal Yabumi",
  "description": "Set Yabumi's expiration to forever automatically.",
  "version": "1.0",
  "icons":{
    "16":"icons/16.png",
    "32":"icons/48.png",
    "128":"icons/128.png"
  },
  "content_scripts":[
    {
      "matches":[
        "https://yabumi.cc/*"
      ],
      "js": [
        "loader.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "setter.js"
  ],
  "permissions": [
    "https://yabumi.cc/*"
  ]
}
